<li class="waves-effect">
  <div class="valign-wrapper">
    <div class="valign-wrapper-name">
    <i class='bx bx-folder list__icon'></i>
    <a href="/lists/{{_id}}">
      <h3>{{listName}}</h3>
    </a>
    </div>
    <div class="valign-wrapper-img {{image}}">
      
    </div>
  </div>
  <div class="valign-wrapper-row">
    <div class="title" id="{{_id}}">
      <span>{{createdAt}}</span>
    </div>
    <div class="card-small-link-container-list">
      <a href="/lists/{{_id}}/edit" class="submit-btn-small-card">Edit</a>
    </div>
    <div class="card-small-link-container-list">
      <a href="/lists/{{_id}}/delete" class="submit-btn-small-card">Delete</a>
    </div>
  </div>
  </div>
  </div>
</li>


