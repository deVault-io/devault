<div class="card-small-container-wrap">
  <div class="card-small-container-list" id="{{_id}}">
    <div class="card-small-img-container-list {{image}}">
     
    </div>
    <div class="card-small-container-list-name">
      <h3>{{listName}}</h3>
    </div>
    <div class="card-small-content-container-list">
      <div class="card-small-linkandfav-container-list">
        <div class="card-small-link-container-list">
          <a href="{{_id}}/add" class="submit-btn-small-card">Add Fav to List</a> 
        </div>
      </div>
    </div>
  </div>
</div>
